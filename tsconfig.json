{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": [
      "ES2020",
      "ES2021.String"
    ],
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "strict": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.test.tsx",
    "src/setupTests.ts"
  ],
  "extends": "expo/tsconfig.base"
}
